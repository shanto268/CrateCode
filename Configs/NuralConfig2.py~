"""                                                                             
This example config file tells which ADC channels are connected
"""

def configureDAQ(h):
    config = dict()
    config["connected_channels"] = ((17, 0), (17, 1), (17, 2), (17, 3), (17, 4),
                                    (17, 5), (17, 6), (17, 7), (17, 8), (17, 9),
                                    (17, 10), (17, 11))
    return config
